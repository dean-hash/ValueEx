{
  "name": "valueex",
  "version": "1.0.0",
  "description": "ValueEx - Intelligent Supply-Demand Matching System",
  "main": "src/index.ts",
  "private": true,
  "scripts": {
    "start": "ts-node src/index.ts",
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "husky install",
    "preview": "vite preview"
  },
  "dependencies": {
    "@prisma/client": "^5.7.0",
    "axios": "^1.6.2",
    "cheerio": "^1.0.0-rc.12",
    "dotenv": "^16.4.7",
    "imapflow": "^1.0.147",
    "mailparser": "^3.6.5",
    "openai": "^4.20.1",
    "typescript": "^5.3.3",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.10",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.0",
    "prisma": "^5.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "vite": "^5.0.7"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
